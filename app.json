{
  "expo": {
    "name": "React-native-playground",
    "slug": "react-native-playground",
    "icon": "./assets/icon.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.webnrolls.erozone",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.webnrolls.erozone",
      "versionCode": 1
    }
  }
}
